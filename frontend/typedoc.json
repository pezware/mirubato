{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./src/modules",
    "./src/components",
    "./src/utils",
    "./src/contexts",
    "./src/services"
  ],
  "entryPointStrategy": "expand",
  "out": "./public/docs",
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/__mocks__/**"
  ],
  "includeVersion": true,
  "name": "Mirubato API Documentation",
  "navigationLinks": {
    "GitHub": "https://github.com/pezware/mirubato",
    "App": "https://mirubato.com",
    "Main Docs": "https://mirubato.com/docs"
  },
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "disableSources": false,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "groupOrder": [
    "Modules",
    "Components",
    "Services",
    "Utilities",
    "Contexts",
    "Types",
    "Interfaces",
    "Classes",
    "Enums",
    "Variables",
    "Functions"
  ],
  "categoryOrder": [
    "Core",
    "Infrastructure",
    "Practice",
    "Performance",
    "Analytics",
    "Curriculum",
    "Visualization",
    "Audio",
    "UI Components",
    "Services",
    "Utilities",
    "Other"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  }
}
