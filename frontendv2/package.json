    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.3",
    "react-image-crop": "^11.0.10",
    "react-pdf": "^10.0.1",
    "react-router-dom": "^7.6.1",
    "swr": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tone": "^14.7.77",
    "uuid": "^11.1.0",
    "vexflow": "^5.0.0",
    "zustand": "^4.5.0"
